{
  "name": "bits-of-reason",
  "namespace": false,
  "reason": { "react-jsx": 3 },
  "refmt": 3,
  "bs-dependencies": ["reason-react", "bs-axios", "re-formality"],
  "ppx-flags": ["re-formality/ppx"],
  "sources": [
    { "dir": "common" },
    { "dir": "bindings" },
    { "dir": "pages", "subdirs": true },
    { "dir": "templates" },
    { "dir": "components", "subdirs": true },
    { "dir": "styles", "subdirs": true },
    "layouts"
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors", "-bs-g"],
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}
